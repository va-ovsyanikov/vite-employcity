        "vue-router": "^4.0.12",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^1.9.3",
        "autoprefixer": "^10.4.0",
        "eslint": "^8.3.0",
        "eslint-plugin-vue": "^8.1.1",
        "json-server": "^0.17.0",
        "postcss": "^8.4.4",
        "prettier": "^2.5.0",
        "tailwindcss": "^2.2.19",
        "vite": "^2.6.4"
    }
}
